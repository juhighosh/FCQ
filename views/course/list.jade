extends ../layout.jade

block content         
 

    div.row
        div.one.column
            | Year
        div.one.column
            | Term
        div.two.columns
            | Course
        div.three.columns
            | Instructor
        div.four.columns
            | Buff Score        

    each item in course
        div.row
            div.one.column
                include field_year
            div.one.column
                include field_term
            div.two.columns
                include field_name
            div.four.columns
                include field_instructors
            div.two.columns
                include field_grade
            svg(width='50', height='50')
             rect(x='0', y='0', width='50', height='50', fill='green')
